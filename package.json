{
  "name": "sbm-poc",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:single": "allure generate allure-results --clean -o allure-report-single --single-file",
    "report:zip": "npm run allure:generate && npm run allure:single && powershell -ExecutionPolicy Bypass -File simple-zip-creator.ps1",
    "report:zip-open": "npm run allure:generate && npm run allure:single && powershell -ExecutionPolicy Bypass -File simple-zip-creator.ps1 -OpenFolder"
  },
